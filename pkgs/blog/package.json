{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "dev": "next dev",
    "ci": "biome ci ./src",
    "check": "run-s check:*",
    "check:lint": "biome check ./src --formatter-enabled=false",
    "check:style": "biome format ./src --diagnostic-level=warn",
    "fix": "run-s fix:*",
    "fix:lint": "pnpm check:lint --apply",
    "fix:style": "pnpm check:style --write",
    "sb": "storybook dev -p 6008",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.0",
    "@vanilla-extract/css": "^1.14.0",
    "@vanilla-extract/next-plugin": "^2.3.2",
    "async-mutex": "^0.5.0",
    "blog-processor": "*",
    "classnames": "^2.5.0",
    "highlight.js": "^11.9.0",
    "next": "14.2.11",
    "next-mdx-remote": "^4.4.1",
    "postcss-import": "^16.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "the-new-css-reset": "^1.11.2",
    "twemoji": "^14.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "npm-run-all2": "^6.0.0",
    "typescript": "^5.3.3"
